<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key='Dark'>
			<SolidColorBrush x:Key='PrimaryBrush'>#0F0F0F</SolidColorBrush>
			<SolidColorBrush x:Key='SecondaryBrush'>#181818</SolidColorBrush>
			<SolidColorBrush x:Key='TernaryBrush'>#262626</SolidColorBrush>
			<SolidColorBrush x:Key='TernaryTranslucentBrush'>#66262626</SolidColorBrush>
			<SolidColorBrush x:Key='HighlightBrush'>#0174DC</SolidColorBrush>
			<SolidColorBrush x:Key='ForegroundBrush'>#FFFFFF</SolidColorBrush>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>